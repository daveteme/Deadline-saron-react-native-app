{
   "expo": {
    "name": "immy",
    "slug": "immy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png", 
    "splash": {
      "image": null,
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.immy.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.immy.app"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}